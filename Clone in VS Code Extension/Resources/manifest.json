{
    "name": "Clone in VS Code",
    "description": "Clone any GitHub, GitLab or Bitbucket repository in Visual Studio Code",
    "version": "1.1.0",
    "manifest_version": 3,
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
      },
    "permissions":[
        "storage"
    ],
    "icons": { "16": "16.png",
           "48": "48.png",
          "128": "128.png" },
    "action": {},
    "author": "infinitepower18",
    "homepage_url": "https://github.com/infinitepower18/CloneInVSCode-Safari",
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions":[
        "https://github.com/*",
        "https://gitlab.com/*",
        "https://bitbucket.org/*"
    ]
  }
